{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "./integration/**/*",
    "./support/**/*",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "declarations.d.ts",
    "./src/types"
  ],
  "compilerOptions": {
    "isolatedModules": false,
    "target": "es5",
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "vite/client"
    ]
  },
}